    "@wdio/sync": "^6.0.14",
    "lerna": "^2.2.0",
    "tslint": "^5.5.0",
    "tslint-loader": "^3.4.3",
    "typescript": "^3.9.2"
  },
  "workspaces": [
    "workflow/*"
  ]
}
