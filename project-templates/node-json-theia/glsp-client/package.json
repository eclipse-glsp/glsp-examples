{
  "private": true,
  "name": "parent",
  "version": "1.0.0",
  "engines": {
    "yarn": ">=1.7.0 <2.x.x",
    "node": ">= 12.14.1"
  },
  "license": "(EPL-2.0 OR GPL-2.0 WITH Classpath-exception-2.0)",
  "author": {
    "name": "Eclipse GLSP"
  },
  "homepage": "https://www.eclipse.org/glsp/",
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipse-glsp/glsp-examples.git"
  },
  "bugs": "https://github.com/eclipse-glsp/glsp/issues",
  "contributors": [
    {
      "name": "Eclipse GLSP Project",
      "email": "glsp-dev@eclipse.org",
      "url": "https://projects.eclipse.org/projects/ecd.glsp"
    }
  ],
  "scripts": {
    "prepare": "lerna run prepare",
    "watch": "lerna run --parallel watch",
    "build": "yarn simpleInstall && lerna run build",
    "lint": "lerna run lint --",
    "simpleInstall": "yarn install --ignore-scripts",
    "rebuild:browser": "theia rebuild:browser",
    "start": "yarn rebuild:browser && yarn --cwd tasklist-browser-app start",
    "start:external": "yarn rebuild:browser && yarn --cwd tasklist-browser-app start:external"
  },
  "devDependencies": {
    "@eclipse-glsp/config": "^1.0.0",
    "lerna": "^4.0.0",
    "typescript": "^4.5.2"
  },
  "workspaces": [
    "tasklist-glsp",
    "tasklist-theia",
    "tasklist-browser-app"
  ],
  "resolutions": {
    "**/@theia/core": "1.26.0",
    "**/@theia/editor": "1.26.0",
    "**/@theia/filesystem": "1.26.0",
    "**/@theia/messages": "1.26.0",
    "**/@theia/monaco": "1.26.0"
  }
}
