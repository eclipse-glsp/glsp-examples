    }
  ],
  "scripts": {
    "prepare": "lerna run prepare",
    "watch": "lerna run --parallel watch",
    "build": "yarn install:only && lerna run build",
    "lint": "lerna run lint --",
    "install:only": "yarn install --ignore-scripts",
    "rebuild:browser": "theia rebuild:browser",
    "start": "yarn rebuild:browser && yarn --cwd tasklist-browser-app start",
    "start:external": "yarn rebuild:browser && yarn --cwd tasklist-browser-app start:external"
  },
  "devDependencies": {
    "@eclipse-glsp/config": "1.1.0-next.d03ad9f.112",
    "lerna": "^6.1.0",
    "typescript": "^4.9.3"
  },
  "workspaces": [
    "tasklist-glsp",
    "tasklist-theia",
    "tasklist-browser-app"
  ],
  "resolutions": {
    "**/@theia/core": "1.34.0",
    "**/@theia/editor": "1.34.0",
    "**/@theia/filesystem": "1.34.0",
    "**/@theia/messages": "1.34.0",
    "**/@theia/monaco": "1.34.0",
    "**/@theia/cli": "1.34.0"
